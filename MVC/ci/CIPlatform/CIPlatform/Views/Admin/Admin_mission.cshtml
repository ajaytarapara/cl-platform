                            <li class="dropdownnavbarlinks"><button class="dropdown-item" type="button"><a
                            style="text-decoration:none;color:black;" href="@Url.Action(" logout", "Account"
                            )">Logout</a></button></li>
                            <li class="dropdownnavbarlinks"><button class="dropdown-item" type="button"><a
                            style="text-decoration:none;color:black;" href="@Url.Action("
                            VolunteeringTimesheet", "Account" )">VolunteeringTimesheet</a>
                            </button></li>*@
                    </ul>
                </div>
            </div>
            <div class="container mt-3">
                <h1>
                    Mission
                </h1>
                <hr>
                <div class="row mt-4" style="justify-content: space-between;">
                    <div class="col-3 px-1"
                         style="align-items: center;display: flex;justify-content:space-between;flex-direction: row;border:1px solid gray;height:50px;border-radius: 10px;margin-left:10px;">
                        <img src="./images/search.png" style="height:20px;">
                        <input type="text" name="" id="search_Mission_adminbtn" placeholder="Search" style="width:90%;border: none;">
                    </div>
                    <div class="col-5">
                    </div>
                    <div class="col-3 d-flex" style="justify-content: flex-end;">
                        <button class="p-2"
                                style="background: none;border: 2px solid orange;border-radius: 15px;
                               color: orange;font-weight: bolder;width: 150px;margin-right: 0px;">
                            <a href="@Url.Action("Admin_Add_Mission","Admin")" 
                            style="text-decoration: none;color: orange;">+ Add</a>
                        </button>
                    </div>
                </div>
                <div class="row mt-4">
                    <div class="table-responsive" id="missiondatalist">

                    </div>
                </div>
